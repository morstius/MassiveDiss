#version 330 core

// Input data
//in vec2 fragmentColor;

// Interpolated values from the vertex shaders
in vec2 UV;

// Ouput data
out vec3 color;

// Values that stay constant for the whole mesh.
uniform sampler2D myTextureSampler;

void main()
{

	// Output color 
	color = texture( myTextureSampler, UV ).rgb; //fragmentColor;

}